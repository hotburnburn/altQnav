{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "altqnav",
  "version": "0.1.0",
  "identifier": "com.tauri-app.altqnav",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "altqnav",
        "width": 500,
        "height": 200,
        "center": true,
        "decorations": false,
        "transparent": false,
        "alwaysOnTop": true,
        "visible": false,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}